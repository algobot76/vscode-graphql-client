{
  "name": "vscode-graphql-client",
  "displayName": "GraphQL Client",
  "description": "GraphQL client for Visual Studio Code",
  "version": "0.0.1",
  "author": {
    "name": "Kaitian Xie",
    "email": "xkaitian@gmail.com"
  },
  "publisher": "algobot76",
  "repository": {
    "type": "git",
    "url": "https://github.com/algobot76/vscode-graphql-client"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "http",
    "keybindings",
    "multi-root ready",
    "graphql",
    "snippet"
  ],
  "activationEvents": [
    "onCommand:graphql-client.request"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "graphql-client.request",
        "title": "GraphQL Client: Send request"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.13",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "@types/graphql": "^14.2.3",
    "@types/highlight.js": "^9.12.3",
    "code-highlight-linenums": "^0.2.1",
    "graphql": "^14.4.2",
    "graphql-formatter": "^1.0.3",
    "graphql-request": "^1.8.2",
    "highlight.js": "^9.15.8",
    "lodash": "^4.17.15"
  }
}
